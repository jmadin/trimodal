<%= form_for(@spawner) do |f| %>
  <% if @spawner.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@spawner.errors.count, "error") %> prohibited this spawner from being saved:</h2>

      <ul>
      <% @spawner.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
